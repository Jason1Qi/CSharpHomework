using System;
    public class Prime
    {
        static void Main(string[] args)
        {
            int userIntput = Convert.ToInt32(Console.ReadLine());
        System.Collections.ArrayList output = new System.Collections.ArrayList();
        for(int i=1;i*i<= userIntput; i++)
        {
            while (userIntput % i == 0)
            {
                userIntput /= i;
                output.Add(i);
            }
        }
        for (int i = 1; i <= output.Capacity; i++)
            Console.WriteLine(output[i].ToString());
        }
    }
